		<Element Type="SqlDmlTrigger" Name="[dbo].[BookAuthors_AfterDelete]">
			<Property Name="IsDeleteTrigger" Value="True" />
			<Property Name="SqlTriggerType" Value="2" />
			<Property Name="BodyScript">
				<Value><![CDATA[
	UPDATE [Authors]
	SET [NumberOfBooks] = [NumberOfBooks] - [DeletedBooksCount]
	FROM (
		SELECT [AuthorID], COUNT([BookID]) AS [DeletedBooksCount]
		FROM deleted 
		GROUP BY [AuthorID]) [DeleteInfo]
	WHERE [Authors].[AuthorID] = [DeleteInfo].[AuthorID]]]></Value>
